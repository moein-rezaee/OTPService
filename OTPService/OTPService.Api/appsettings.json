{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cache": {
    "DefaultProvider": "Memory",
    "DefaultExpirationMinutes": 5,
    "Providers": {
      "Redis": {
        "Host": "redis",
        "Port": 6379,
        "Username": "${REDIS_USERNAME}",
        "Password": "${REDIS_PASSWORD}"
      }
    }
  },
  "Notification": {
    "BaseUrl": "${NOTIFICATION_BASE_URL}"
  }
}
