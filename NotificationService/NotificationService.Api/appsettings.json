{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  
  "Sms": {
    "DefaultProvider": "Farapayamak",
    "Providers": {
      "Kavenegar": {
        "ApiKey": "${KAVENEGAR_API_KEY}",
        "Sender": "${KAVENEGAR_SENDER}"
      },
      "Farapayamak": {
        "Username": "${FPP_USERNAME}",
        "Password": "${FPP_PASSWORD}",
        "From": "${FPP_FROM}"
      }
    }
  }
}
